#include <stdio.h>

void solve(int rows, int cols, int matrix[rows][cols])
{
    for (int i = 0; i < rows; i++)
    {
        for (int j = 0; j < cols; j++)
        {
            printf("%d ", matrix[i][j]);
        }
        printf("\n");
    }
}

int main()
{
    int rows = 3;
    int cols = 3;

    int matrix[rows][cols] =
    {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };

    solve(rows, cols, matrix);

    return 0;
}

